Views :
from django.shortcuts import render, redirect, HttpResponse
from .models import Register, Donor, OrganSuitabilityCriteria, Recipient,Transplant

def home(request):
    return render(request, 'home.html')

def register(request):
    if request.method == 'POST':
        getname = request.POST.get('Name')
        getage = request.POST.get('Age')
        getgender = request.POST.get('Gender')
        getblood_type = request.POST.get('Blood_type')
        getrole = request.POST.get('role')
        getusername = request.POST.get('Username')
        getpassword = request.POST.get('Password')
        getcontact_number = request.POST.get('Contact_number')
        getemail = request.POST.get('Email')
        getaddress = request.POST.get('Address')
        getorgan_to_donate = request.POST.get('Organ_to_donate')
        getorgan_to_transplant = request.POST.get('Organ_to_transplant')

        users = Register()
        users.Name = getname
        users.Age = getage
        users.Gender = getgender
        users.Blood_type = getblood_type
        users.role = getrole
        users.Username = getusername
        users.Password = getpassword
        users.Contact_number = getcontact_number
        users.Email = getemail
        users.Address = getaddress
        users.Organ_to_donate = getorgan_to_donate
        users.Organ_to_transplant = getorgan_to_transplant
        users.save()
    return render(request, 'register.html')

def donor(request):
    if request.method == 'POST':
        getname = request.POST.get('Name')
        getage = request.POST.get('Age')
        getblood_type = request.POST.get('Blood_type')
        getorgan_type = request.POST.get('Organ_type')
        getmedical_history = request.POST.get('Medical_history')
        gethealth_status = request.POST.get('Health_status')
        donor = Donor()
        donor.Name = getname
        donor.Age = getage
        donor.Blood_type = getblood_type
        donor.Organ_type = getorgan_type
        donor.Medical_history = getmedical_history
        donor.Health_status = gethealth_status
        donor.save()
    return render(request, 'donor.html')
def organsuitabilitycriteria(request):
    if request.method == 'POST':
        getorgan_type = request.POST.get('Organ_type')
        getcompatibility_score = request.POST.get('Compatibility_score')
        getmedical_condition = request.POST.get('Medical_condition')
        getage_limit = request.POST.get('Age_limit')
        getdonor_blood_type = request.POST.get('Donor_blood_type')
        getwaiting_time_limit = request.POST.get('Waiting_time_limit')
        getadditional_criteria = request.POST.get('Additional_criteria')

        organ = OrganSuitabilityCriteria()
        organ.Organ_type = getorgan_type
        organ.Compatibility_score = getcompatibility_score
        organ.Medical_condition = getmedical_condition
        organ.Age_limit = getage_limit
        organ.Donor_blood_type = getdonor_blood_type
        organ.Waiting_time_limit = getwaiting_time_limit
        organ.Additional_criteria = getadditional_criteria
        organ.save()

    return render(request, 'organ.html')

def recipient(request):
    if request.method == 'POST':
        getname = request.POST.get('Name')
        getage = request.POST.get('Age')
        getgender = request.POST.get('Gender')
        getoverall_health = request.POST.get('Overall_health')
        getblood_type = request.POST.get('Blood_type')
        getorgan_type = request.POST.get('Organ_type')
        gethiv_status = request.POST.get('Hiv_status')
        gethbsag_status = request.POST.get('Hbsag_status')
        gettissue_compatibility = request.POST.get('Tissue_compatibility')
        getimmune_system_assessment = request.POST.get('Immune_system_assessment')
        getwaiting_list_criteria = request.POST.get('Waiting_list_criteria')
        getpsychosocial_assessment = request.POST.get('Psychosocial_assessment')
        getfinancial_implications = request.POST.get('Financial_implications')
        getsupport_systems = request.POST.get('Support_systems')
        getregistration_date = request.POST.get('Registration_date')  # Remove extra space
        gettransplantation_history = request.POST.get('Transplantation_history')
        getcurrent_medications = request.POST.get('Current_medications')
        getpast_medical_history = request.POST.get('Past_medical_history')
        getallergies = request.POST.get('Allergies')
        getcontact_information = request.POST.get('Contact_information')
        getemergency_status = request.POST.get('Emergency_status')

        recipient = Recipient()
        recipient.Name = getname
        recipient.Age = getage
        recipient.Gender = getgender
        recipient.Overall_health = getoverall_health
        recipient.Blood_type = getblood_type
        recipient.Organ_type = getorgan_type
        recipient.Hiv_status = gethiv_status
        recipient.Hbsag_status = gethbsag_status
        recipient.Tissue_compatibility = gettissue_compatibility
        recipient.Immune_system_assessment = getimmune_system_assessment
        recipient.Waiting_list_criteria = getwaiting_list_criteria
        recipient.Psychosocial_assessment = getpsychosocial_assessment
        recipient.Financial_implications = getfinancial_implications
        recipient.Support_systems = getsupport_systems
        recipient.Registration_date = getregistration_date
        recipient.Transplantation_history = gettransplantation_history
        recipient.Current_medications = getcurrent_medications
        recipient.Past_medical_history = getpast_medical_history
        recipient.Allergies = getallergies
        recipient.Contact_information = getcontact_information
        recipient.Emergency_status = getemergency_status
        recipient.save()
    return render(request, 'recipient.html')

def adminlogin(request):
    if request.method == 'POST':
        getusername = request.POST.get('username')
        getpassword = request.POST.get('password')
        if getusername == 'admindera' and getpassword == 'dera@123':
            # Redirect to the admin home page if the login is successful
            return redirect('/adminhome/')
        else:
            return HttpResponse('Invalid credentials')
    return render(request,'adminlogin.html')


def adminhome(request):
    return render(request, 'adminhome.html')

def pending_donor(request):
    donor_details = Donor.objects.filter(Status=0)  # 0 corresponds to 'Pending'
    return render(request, 'pending_donor.html', {'donor_details': donor_details})

def pending_organ(request):
    organ_details = OrganSuitabilityCriteria.objects.filter(Status=0)
    return render(request, 'pending_organ.html', {'organ_details': organ_details})

def pending_recipient(request):
    recipient_details = Recipient.objects.filter(Status=0)
    return render(request, 'pending_recipient.html', {'recipient_details': recipient_details})

def approved_donor(request):
    donor_details = Donor.objects.filter(Status=1)  # 1 corresponds to 'Approved'
    return render(request, 'approved_donor.html', {'donor_details': donor_details})

def approved_organ(request):
    organ_details = OrganSuitabilityCriteria.objects.filter(Status=1)
    return render(request, 'approved_organ.html', {'organ_details': organ_details})

def approved_recipient(request):
    recipient_details = Recipient.objects.filter(Status=1)
    return render(request, 'approved_recipient.html', {'recipient_details': recipient_details})

def approve(request, donor_id, organ_id, recipient_id):
    donor_entry = Donor.objects.get(id=donor_id)
    donor_entry.Status = 1  # 1 corresponds to 'Approved'
    donor_entry.save()

    organ_entry = OrganSuitabilityCriteria.objects.get(id=organ_id)
    organ_entry.Status = 1
    organ_entry.save()

    recipient_entry = Recipient.objects.get(id=recipient_id)
    recipient_entry.Status = 1
    recipient_entry.save()

    # Determine the type of approval and redirect accordingly
    if donor_id:
        return redirect('pending_donor')
    elif organ_id:
        return redirect('pending_organ')
    elif recipient_id:
        return redirect('pending_recipient')
    else:
        # Handle the case when none of donor_id, organ_id, recipient_id is provided
        return redirect('adminhome')

def pending(request):
    donor_details = Donor.objects.filter(Status=0)  # 0 corresponds to 'Pending'
    organ_details = OrganSuitabilityCriteria.objects.filter(Status=0)
    recipient_details = Recipient.objects.filter(Status=0)

    return render(request, 'pendinglist.html', {'donor_details': donor_details, 'organ_details': organ_details,
                                                'recipient_details': recipient_details})

def approve(request, donor_id, organ_id, recipient_id):
    donor_entry = Donor.objects.get(id=donor_id)
    donor_entry.Status = 1  # 1 corresponds to 'Approved'
    donor_entry.save()

    organ_entry = OrganSuitabilityCriteria.objects.get(id=organ_id)
    organ_entry.Status = 1
    organ_entry.save()

    recipient_entry = Recipient.objects.get(id=recipient_id)
    recipient_entry.Status = 1
    recipient_entry.save()

    return redirect('/pending')

def approved(request):
    donor_details = Donor.objects.filter(Status=1)  # 1 corresponds to 'Approved'
    organ_details = OrganSuitabilityCriteria.objects.filter(Status=1)
    recipient_details = Recipient.objects.filter(Status=1)

    return render(request, 'approvedlist.html', {'donor_details': donor_details, 'organ_details': organ_details,
                                                 'recipient_details': recipient_details})

def transplant(request):
    if request.method == 'POST':
        getdonor = request.POST.get('donor.Name')
        getrecipient = request.POST.get('recipient.Name')
        getorgan_type = request.POST.get('organ_type.Name')
        gettransplant_date = request.POST.get('transplant_date.Name')
        getsuccess = request.POST.get('success.Name')

        transplant = Transplant()
        transplant.Donor = getdonor
        transplant.Recipient = getrecipient
        transplant.Organ_type = getorgan_type
        transplant.Transplant_date = gettransplant_date
        transplant.Success = getsuccess
        transplant.save()
    return render(request, 'transplant.html')

def transplant_list(request):
    transplant_list = Transplant.objects.all()
    return render(request, 'transplant_list.html', {'transplant_list': transplant_list})


def aboutus(request):
    return render(request, 'aboutus.html')
