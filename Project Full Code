        <th>Transplant Date</th>
        <th>Success</th>
    </tr>
    {% for transplant in transplant_list %}
        <tr>
            <td>{{ transplant.Donor }}</td>
            <td>{{ transplant.Recipient }}</td>
            <td>{{ transplant.Organ_type }}</td>
            <td>{{ transplant.Transplant_date }}</td>
            <td>{{ transplant.Success }}</td>
        </tr>
    {% endfor %}
</table>

