                    <td>{{ recipient.Organ_type }}</td>
                    <td>{{ recipient.HIV_status }}</td>
                    <td>{{ recipient.Hbsag_status }}</td>
                    <td>{{ recipient.Tissue_compatibility }}</td>
                    <td>{{ recipient.Immune_system_assessment }}</td>
                    <td>{{ recipient.Waiting_list_criteria }}</td>
                    <td>{{ recipient.Psychosocial_assessment }}</td>
                    <td>{{ recipient.Support_systems }}</td>
                    <td>{{ recipient.Registration_date }}</td>
                    <td>{{ recipient.Transplantation_history }}</td>
                    <td>{{ recipient.Current_medications }}</td>
                    <td>{{ recipient.Past_medical_history }}</td>
                    <td>{{ recipient.Allergies }}</td>
                    <td>{{ recipient.Contact_information }}</td>
                    <td>{{ recipient.Emergency_status }}</td>
                    <td>{{ recipient.Status|default:"Pending" }}</td>
