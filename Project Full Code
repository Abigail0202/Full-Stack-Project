 <center><h1>Donor Organ Details</h1></center>
    <form method="post">
        {% csrf_token %}

        <h2>Donor Organ Details</h2>
        <table border="1">
            <tr>
                <th>Organ Type</th>
                <th>Compatibility Score</th>
                <th>Medical Condition</th>
                <th>Age Limit</th>
                <th>Donor Blood Type</th>
                <th>Waiting Time Limit</th>
                <th>Additional Criteria</th>
                <th>Status</th>
                <th>Approval</th>
            </tr>
            {% for organ in organ_details %}
                <tr>
                    <td>{{ organ.Organ_type }}</td>
                    <td>{{ organ.Compatibility_score }}</td>
                    <td>{{ organ.Medical_condition }}</td>
                    <td>{{ organ.Age_limit }}</td>
                    <td>{{ organ.Donor_blood_type }}</td>
                    <td>{{ organ.Waiting_time_limit }}</td>
                    <td>{{ organ.Additional_criteria }}</td>
                    <td>{{ organ.Status|default:"Pending" }}</td>

                    <td><a href="/approve/{{ organ.id }}">Doctors Approval</a></td>
                </tr>
            {% endfor %}
