Models: 
from django.db import models


class Register(models.Model):
    ROLE_CHOICES = [
        ('donor', 'Donor'),
        ('patient', 'Patient'),
    ]

    BLOOD_TYPE_CHOICES = [
        ('O', 'O'),
        ('A', 'A'),
        ('B', 'B'),
        ('AB', 'AB'),
        ('0-', 'O-'),
        ('A-', 'A-'),
        ('B-', 'B-'),
        ('AB-', 'AB-'),
    ]

    ORGAN_CHOICES = [
        ('kidney', 'Kidney'),
        ('heart', 'Heart'),
        ('lungs', 'lungs'),
    ]

    Name = models.CharField(max_length=100)
    Age = models.IntegerField()
    Gender = models.CharField(max_length=10, choices=[('male', 'Male'), ('female', 'Female'), ('other', 'Other')])
    Blood_type = models.CharField(max_length=5)
    role = models.CharField(max_length=10, choices=ROLE_CHOICES)
    Username = models.CharField(max_length=20)
    Password = models.CharField(max_length=20)
    Contact_number = models.CharField(max_length=15)
    Email = models.EmailField()
    Address = models.CharField(max_length=20)
    Organ_to_donate = models.CharField(max_length=25, choices=ORGAN_CHOICES)
    Organ_to_transplant = models.CharField(max_length=25, choices=ORGAN_CHOICES)

class Donor(models.Model):
    Name = models.CharField(max_length=100)
    Age = models.IntegerField()
    Blood_type = models.CharField(max_length=10)
    Organ_type = models.CharField(max_length=100)
    Medical_history = models.CharField(max_length=255)
    Health_status = models.CharField(max_length=100)
    Status = models.IntegerField(choices=[(0, 'Pending'), (1, 'Approved')], default=0)


class OrganSuitabilityCriteria(models.Model):
    Organ_type = models.CharField(max_length=50, unique=True)
    Compatibility_score = models.IntegerField()
    Medical_condition = models.CharField(max_length=100)
    Age_limit = models.IntegerField()
    Donor_blood_type = models.CharField(max_length=5)
    Waiting_time_limit = models.IntegerField()
    Additional_criteria = models.TextField()
    Status = models.IntegerField(choices=[(0, 'Pending'), (1, 'Approved')], default=0)


class Recipient(models.Model):
    BLOOD_TYPE_CHOICES = [
        ('O', 'O'),
        ('A', 'A'),
        ('B', 'B'),
        ('AB', 'AB'),
        ('0-', 'O-'),
        ('A-', 'A-'),
        ('B-', 'B-'),
        ('AB-', 'AB-'),

    ]

    Name = models.CharField(max_length=100)
    Age = models.IntegerField()
    Gender = models.CharField(max_length=10, choices=[('male', 'Male'), ('female', 'Female'), ('other', 'Other')])
    Overall_health = models.CharField(max_length=255)
    Blood_type = models.CharField(max_length=10, choices=BLOOD_TYPE_CHOICES)
    Organ_type = models.CharField(max_length=15)
    Hiv_status = models.CharField(max_length=10, choices=[('Negative', 'Negative'), ('Positive', 'Positive')])
    Hbsag_status = models.CharField(max_length=10, choices=[('Negative', 'Negative'), ('Positive', 'Positive')])
    Tissue_compatibility = models.CharField(max_length=255)
    Immune_system_assessment = models.CharField(max_length=255)
    Waiting_list_criteria = models.IntegerField()
    Psychosocial_assessment = models.CharField(max_length=100)
    Financial_implications = models.CharField(max_length=100)
    Support_systems = models.CharField(max_length=100)
    Registration_date = models.DateField(auto_now_add=True)
    Transplantation_history = models.TextField()
    Current_medications = models.TextField()
    Past_medical_history = models.TextField()
    Allergies = models.CharField(max_length=100)
    Contact_information = models.CharField(max_length=255)
    Emergency_status = models.CharField(max_length=255)
    Status = models.IntegerField(choices=[(0, 'Pending'), (1, 'Approved')], default=0)


class Transplant(models.Model):
    Donor = models.CharField(max_length=255)
    Recipient = models.CharField(max_length=255)
    Organ_type = models.CharField(max_length=255)
    Transplant_date = models.DateField()
    Success = models.CharField(max_length=255)

