Urls:
from django.urls import path
from . import views

urlpatterns = [
    path('', views.home),
    path('home/', views.home),
    path('register/', views.register),
    path('donor/', views.donor),
    path('organ/', views.organsuitabilitycriteria),
    path('recipient/', views.recipient),
    path('adminlogin/', views.adminlogin),
    path('adminhome/', views.adminhome),
    path('pendingdonor/', views.pending_donor),
    path('pendingorgan/', views.pending_organ),
    path('pendingrecipient/', views.pending_recipient),
    path('approveddonor/', views.approved_donor),
    path('approvedorgan/', views.approved_organ),
    path('approvedrecipient/', views.approved_recipient),
    path('pending/', views.pending),
    path('approve/<int:donor_id>/<int:organ_id>/<int:recipient_id>/', views.approve),
    path('approved/', views.approved),
    path('transplant/', views.transplant),
    path('transplantlist/', views.transplant_list),
    path('about/',views.aboutus)

]
